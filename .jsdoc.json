{

  "tags": {
    "allowUnknownTags": true
  },
  "opts": {
    "destination": "./api-docs/gen"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "systemName":"Plasmatic",
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true
    },
    "path": "ink-docstrap",
    "theme": "flatly",
    "navType": "inline",
    "outputSourceFiles": true,
    "syntaxTheme": "dark",
    "linenums": true,
    "dateFormat": "MMMM Do YYYY, h:mm:ss a"
  }
}
